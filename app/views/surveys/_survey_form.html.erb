<%= render_errors(f) %>

<%= f.input :type, as: :hidden, value: "Survey" %>
<%= f.input :name %>
<%= f.input :description %>
<%= f.input :survey_question %>
<%= f.input :minimum_requirement %>
<%= f.input :target_percentage %>
<%= f.input :archived, as: :select, include_blank: false %>

<div class="form-actions">
  <%= f.submit %>
  <%= cancel_button %>
</div>
