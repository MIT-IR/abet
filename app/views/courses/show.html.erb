<p>Course <%= @course.number %>,&nbsp;<%= @course.name %><p>


//Go back to courses

<table class="listing" summary="Outcome list">
  <col width = "20">
  <col width = "1000"
    <tr class="header">
      <th>&nbsp;</th>
      <th>Outcome</th>
      <th>&nbsp;</th>
    </tr>
    <% @outcomes.each do |out| %>
    <tr>
      <td><%= out.outcome %></td>
      <td><%= out.description %></td>
      <td class="actions">
        <%= link_to("Show", '#', :class => 'action show') %>
        <%= link_to("Edit", '#', :class => 'action edit') %>
        <%= link_to("Delete", '#', :class => 'action delete') %>
      </td>
    </tr>
    <% end %>
  </table>
